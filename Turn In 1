library(ma391)


#PROBLEM 7.5.5

#part b
up=function(x){(x/(12*171))+(2*(x^1/2))/(12*171)}
low=function(x){x/(12*171)+(2*(x^1/2))/(12*171)}

up(2050)

#part c
up=function(x){(2050/(12*x))+(2*(2050^1/2))/(12*x)}
low=function(x){x/(12*171)+(2*(x^1/2))/(12*171)}
up(178.37)

